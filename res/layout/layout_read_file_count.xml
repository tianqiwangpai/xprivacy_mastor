<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:scrollbarStyle="outsideOverlay"
    tools:ignore="HardcodedText,TooManyViews,Overdraw" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:divider="?android:attr/listDivider"
            android:showDividers="middle"
            android:shrinkColumns="2"
            android:stretchColumns="2" >

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/read_file_count_name"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/read_file_count"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/read_file_mode"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskName"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskDesc"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskNewdata"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskNumber"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskReturnratio"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskDeclineFlag"
                    
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskDecilneRatio"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskDecilneMin"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskNextDayFlag"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskNextDayVisitInterval"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskNextDayVisitIntervalReturnRatio"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskNextDayVisitIntervalCount"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskNextDayVisitDeclineFlag"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskNextDayVisitDecilneRatio"
                    android:textIsSelectable="false" />
            </TableRow>

            <TableRow style="@style/PreferenceLine" >

                <TextView
                    android:id="@+id/tvTaskNextDayVisitDecilneMin"
                    android:textIsSelectable="false" />
            </TableRow>
        </TableLayout>
    </LinearLayout>

</ScrollView>